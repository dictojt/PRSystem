APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost
# When using a tunnel (ngrok, devtunnels, etc.): set APP_URL to the tunnel HTTPS URL
# so Google Sign-In and links work. Example ngrok: https://abc123.ngrok-free.app
# For assets: run `npm run build`, or forward both app port and Vite port (5173).

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

# PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=prs
DB_USERNAME=root
DB_PASSWORD=

SESSION_DRIVER=file
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null
# When using ngrok (HTTPS), set to true so the session cookie is sent on the OAuth callback
# SESSION_SECURE_COOKIE=true

# Google OAuth (Sign in with Google on home page)
# Get credentials: https://console.cloud.google.com/ → APIs & Services → Credentials → Create OAuth 2.0 Client ID (Web application)
# When using ngrok: the app uses the current request URL for the callback, so add your ngrok URL to Authorized redirect URIs in Google Console, e.g. https://xxxx.ngrok-free.app/auth/google/callback
# For local only: http://localhost:8000/auth/google/callback
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
# Optional. Override callback URL (default: current request origin + /auth/google/callback when not localhost; else APP_URL + /auth/google/callback)
# GOOGLE_REDIRECT_URI=http://localhost:8000/auth/google/callback

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"
